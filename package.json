{
  "name": "rule-zero",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-three/drei": "^9.48.1",
    "@react-three/postprocessing": "^2.7.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^8.0.2",
    "react": "^18.2.0",
    "react-breakpoints-hook": "^0.1.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "sass": "^1.57.1",
    "three": "^0.146.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "sass-dev": "npx sass --watch --style=expanded src/scss:src/css",
    "sass-prod": "npx sass --watch --no-source-map --style=compressed src/scss:src/css",
    "start": "react-scripts start",
    "build": "react-scripts build && react-compress",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
